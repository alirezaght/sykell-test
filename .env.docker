# Docker Environment Configuration
# Copy this file to .env and modify as needed

# Server Configuration
PORT=7070
ENVIRONMENT=production

# Database Configuration (MySQL)
MYSQL_ROOT_PASSWORD=rootpassword
MYSQL_DATABASE=sykell_db
MYSQL_USER=sykell_user
MYSQL_PASSWORD=sykell_password

# JWT Configuration
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production

# Temporal Configuration
TEMPORAL_NAMESPACE=default

# Logging
LOG_LEVEL=info
LOG_FORMAT=json

# Optional: Override any other environment variables as needed
# DATABASE_URL will be automatically constructed from MYSQL_* variables
# TEMPORAL_HOST_PORT will be set to temporal:7233 in Docker network